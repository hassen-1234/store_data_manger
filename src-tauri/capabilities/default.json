{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text",
    "sql:default",
    "sql:allow-execute",
    "core:window:allow-set-theme"
  ]
}